@model IEnumerable<AIS_AsepKomarudin_Test.Models.ReportProduct>

@{
    ViewBag.Title = "View";
}

<h2>Answer of Number One [1]</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Barcode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jumlah)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Total_harga)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ready)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.onhold)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.delivered)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.packing)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sent)
        </th>
   
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Barcode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jumlah)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Total_harga)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ready)
        </td>
