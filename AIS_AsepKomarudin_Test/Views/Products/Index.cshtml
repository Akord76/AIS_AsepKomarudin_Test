            @Html.DisplayFor(modelItem => item.onhold)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.delivered)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.packing)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sent)
        </td>
     
    </tr>
}

</table>

<script>
    var dataNumber = [1,-1,3,-4,5,-2,7,4,2];

    document.write("<h2>Answer of Number Two 2:</h2>");

    var shortNumber = dataNumber.sort().toString();
    var trimNumber = shortNumber.substring(9);
    var result = trimNumber.slice(0, 4);
    var result1 = trimNumber.slice(6, 7);
    var lastresult = result + result1;
